<% include partials/header%>
<div class="ui header inverted segment centered container fluid">
    <h1>Don't Let these deals slither away!</h1>
    <sub>There is an automatic $25.00 shipping added to every order.</sub>
</div>
<div class="ui six doubling cards container centered">
    <%snakes.forEach(function(snakes){%>
      <div class="card">
    <div class="image">
      <img class="ui rounded"src="<%=snakes.image%>">
    </div>
    <div class="content">
        <%=snakes.discrip%>
    </div>
    <div class="extra">
      Price: $<%=snakes.price%>
    </div>
    <div class="ui">
      <a class="ui button attached bottom black centered"href="/snake/<%=snakes._id%>">View Me</a>
    </div>
  </div>
    <% }); %>
</div>
<% include partials/footer%>